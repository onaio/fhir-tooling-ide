CREATE TABLE IF NOT EXISTS `config` (
    config_id TEXT PRIMARY KEY NOT NULL,
    config_title TEXT NOT NULL,
    body TEXT NOT NULL
);

selectAll:
SELECT * FROM `config`;

insertConfig:
INSERT INTO `config` (config_id, config_title, body)
VALUES (?, ?, ?);

updateById:
UPDATE `config`
SET body = ?
WHERE config_id = ?;

deleteById:
DELETE FROM `config`
WHERE config_id = ?;